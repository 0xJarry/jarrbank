# Server Configuration
NODE_ENV=production
HOST=0.0.0.0
PORT=3001
LOG_LEVEL=info

# Frontend URL for CORS
FRONTEND_URL=https://jarrbank.vercel.app

# Price Data APIs (from Story 1.6)
MORALIS_API_KEY=your_moralis_api_key_here
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# RPC Providers - Backend API
# Get your Alchemy API key from https://www.alchemy.com/
# These URLs are for the backend API to make RPC calls
ALCHEMY_API_KEY=your_alchemy_api_key_here
ALCHEMY_ETH_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your_alchemy_api_key_here
ALCHEMY_ARB_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your_alchemy_api_key_here
ALCHEMY_AVAX_RPC_URL=https://avax-mainnet.g.alchemy.com/v2/your_alchemy_api_key_here

# Get your Infura credentials from https://infura.io/ (fallback provider)
INFURA_PROJECT_ID=your_infura_project_id_here
INFURA_PROJECT_SECRET=your_infura_project_secret_here

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Rate Limiting
RPC_RATE_LIMIT_MAX=100
RPC_RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=100
RATE_LIMIT_TIME_WINDOW=60000

# Cache TTL (in seconds)
CACHE_TTL_BALANCE=30
CACHE_TTL_METADATA=3600

# Database (Supabase)
DATABASE_URL=postgresql://user:password@host:port/database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Security
CORS_CREDENTIALS=true

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn_here
AXIOM_TOKEN=your_axiom_token_here
AXIOM_DATASET=your_axiom_dataset_here